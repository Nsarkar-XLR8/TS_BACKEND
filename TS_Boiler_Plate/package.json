    "mongoose": "^9.2.1",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "nodemailer": "^8.0.1",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.3.6"
