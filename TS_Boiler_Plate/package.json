    "@opentelemetry/sdk-node": "^0.211.0",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.9.0",
    "compression": "^1.8.1",
    "connect-timeout": "^1.9.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
