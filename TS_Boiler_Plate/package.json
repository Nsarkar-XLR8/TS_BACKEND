{
  "name": "ts_boiler_plate",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "dev": "npx kill-port 5001 && tsx watch --clear-screen=false src/server.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.test.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run typecheck && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rmdir /s /q dist 2>nul || exit /b 0",
    "ci": "npm run typecheck && npm run lint && npm run test",
    "prepare": "husky"
  },
